# TYPO3 Scheduler Aliases
alias t3-scheduler-v12='cd /var/www/html/v12 && vendor/bin/typo3 scheduler:run'

# Cache management
alias t3-cache-flush-v12='cd /var/www/html/v12 && vendor/bin/typo3 cache:flush'
alias t3-cache-warmup-v12='cd /var/www/html/v12 && vendor/bin/typo3 cache:warmup'

# Quick TYPO3 commands
alias t3='cd /var/www/html/v12 && vendor/bin/typo3'
